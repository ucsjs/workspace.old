<nav aria-label="Anchors" class="fixed right-48 anchor">
    <ul role="list" id="anchors">
        <% anchors.forEach((item) => { %>
        <li>
            <a href="#<%= item.id %>" class="text-sm font-medium text-gray-500 hover:text-gray-700"><%= item.id %></a>
        </li>
        <% }); %>
    </ul>
</nav>