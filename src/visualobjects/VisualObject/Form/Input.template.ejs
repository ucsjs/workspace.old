<input 
    ref="input<%= id %>"
    class="<%= styleId %>"
    type="<%= Input.type %>" 
    value="<%= Input.value %>"
    <% if(Input.name) { %>name="<%= Input.name %>"<% } %>
    <% if(Input.checked) { %>disabled="<%= Input.checked %>"<% } %>
    <% if(Input.disabled) { %>disabled="<%= Input.disabled %>"<% } %>
    <% if(Input.multiple) { %>multiple="<%= Input.multiple %>"<% } %>
    <% if(Input.readonly) { %>readonly="<%= Input.readonly %>"<% } %>
    <% if(Input.required) { %>required="<%= Input.required %>"<% } %>
    :style="<%= JSON.stringify(style) %>"
/>